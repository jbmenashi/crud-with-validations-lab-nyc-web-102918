<table>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.released %></td>
        <td><%= song.release_year %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
        <td><%= button_to('Destroy', songs_path,
          method: "delete", { confirm: 'Are you sure?', disable_with: 'loading...' }) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
